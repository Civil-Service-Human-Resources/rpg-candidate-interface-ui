<div id="skiplink-container">
    <div>
        <a href="#content" class="skiplink"></a>
    </div>
</div>

{{> messages/cookie-notice showCookieBanner=showCookieWarning }}

<header role="banner" class="global-header global-header--with-proposition">
    <div class="global-header__wrapper">

        <div class="global-header__brand">
            <div class="global-header__logo">
                <a href="https://www.gov.uk/" title="Go to the GOV.UK homepage" class="global-header__logo-link">
                    <img src="/images/icons/gov.uk_logotype_crown_invert_trans.png" width="36" height="32" alt=""> GOV.UK
                </a>
            </div>
        </div>

        <div class="global-header__proposition">
            <a href="/" class="global-header__proposition-name">{{__ 'global.header.title' }}</a>
        </div>

    </div>
</header>

<span class="global-header__accent"></span>

<div class="phase-banner">
    <div class="phase-banner__inner">
        <p>
            <strong class="phase-tag">Beta</strong>
            <span>{{{__ 'global.header.betaText' }}}</span>
        </p>

        <div class="phase-banner__account-links">
            <a href="/internal-jobs">Sign in to access internal jobs</a>
        </div>
    </div>
</div>